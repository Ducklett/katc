fn helloworld() {
	print("hello world!\n")
}

fn greet(name:string, age:int) {
	fn localfunction() { print("!!\n") }
	print("hello %s %d", name, age)
	localfunction()
}

helloworld()
greet("bob", 20)
// functions can optionally be called with the parameter names in front
greet(name: "bob", age: 20)
