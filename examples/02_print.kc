// print behaves just like printf
print("hello\n");

// pass arguments
// int: %d
// bool: %s
// string: %s
// char: %c
print("first letter: %c\n", 'B');
print("name: %s\n", "Bob");
print("age: %d\n", 21);
print("age in hex: %d\n", 0x15);
print("age in binary: %d\n", 0b00010101);
print("registered: %s\n", true);
